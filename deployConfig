octavoTufteSolarisedPdf, "standard dark theme PDF", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc --metadata=solariseddarkon:true --metadata=solariseddarkoff:false --filter="$OCTAVOPATH/filters/filterPopout.py" --filter="$OCTAVOPATH/filters/filterKeyword.py" --filter="$OCTAVOPATH/filters/filterDo.py" $customfilterfour --filter="$OCTAVOPATH/filters/filterDivToTufte.py"  $customfilterone $customfiltertwo $customfilterthree  "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoTuftePdf, "standard PDF", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc  --filter="$OCTAVOPATH/filters/filterPopout.py" --filter="$OCTAVOPATH/filters/filterKeyword.py" --filter="$OCTAVOPATH/filters/filterDo.py" $customfilterfour --filter="$OCTAVOPATH/filters/filterDivToTufte.py"  $customfilterone $customfiltertwo $customfilterthree  "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoTufteBookPdf, "standard book PDF", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc  --filter="$OCTAVOPATH/filters/filterPopout.py" --filter="$OCTAVOPATH/filters/filterKeyword.py" --filter="$OCTAVOPATH/filters/filterDo.py" $customfilterfour --filter="$OCTAVOPATH/filters/filterDivToTufte.py"  $customfilterone $customfiltertwo $customfilterthree  "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoNormalPdf, "simplified layout PDF", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc  $customfilterfour $customfilterone $customfiltertwo $customfilterthree "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoLargePdf, "large text PDF", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc --metadata=large:true $customfilterfour $customfilterone $customfiltertwo $customfilterthree "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoOpenDyslexicPdf, "PDF with Open Dyslexic font", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc --metadata=dyslexicfont:OpenDyslexic3 $customfilterfour $customfilterone $customfiltertwo $customfilterthree "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.pdf",.pdf
octavoHtml, "HTML (webpage) format", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc --mathml $customfilterfour --filter="$OCTAVOPATH/filters/filterDivToHtml.py" $customfilterone $customfiltertwo $customfilterthree "$numberSections" --self-contained --toc --css "$templatedir/octavoHtmlTufte.css" -o "$deployto/$basenameSourceFile$element$mdFive.html",.html
octavoDocx, "Microsoft Word format", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc  $customfilterfour $customfilterone $customfiltertwo $customfilterthree "$numberSections" --template="$templatedir/$element.tex" --latex-engine=xelatex -o "$deployto/$basenameSourceFile$element$mdFive.docx",.docx
octavoSpoken, "spoken format", sed "s@& documentFormat &@$deployDescription[$counter]@g" | pandoc  $customfilterfour --filter="$OCTAVOPATH/filters/filterDivToLatex.py" $customfilterone $customfiltertwo $customfilterthree "$numberSections" | pandoc -f html -t plain | sed 's/_//g' | sed 's/â†©//g' | say --voice='Alex' --quality=127 --file-format='mp4f' -o "$deployto/$basenameSourceFile$element$mdFive.mp4",.mp4
